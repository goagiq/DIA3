                            <li onmouseover="showEnhancedTooltip(event, {
                                title: 'Deterrence Effectiveness',
                                description: 'Assessment of deterrence capabilities and effectiveness.',
                                source: '<strong>📊 Sources:</strong><br/>• Military Analysis<br/>• Strategic Studies<br/>• Capability Assessments<br/>• Historical Data',
                                strategic_impact: '<strong>🎯 Impact:</strong><br/>Critical for maintaining regional stability.',
                                recommendations: 'Enhance deterrence capabilities through strategic partnerships.',
                                use_cases: 'Military planning and strategic defense.'
                            })" onmouseout="hideEnhancedTooltip()">
                                <strong>Deterrence Effectiveness</strong>
                                <p>Assessment of deterrence capabilities and effectiveness</p>
                                <span class="impact-level">Critical Impact</span>
                            </li>
                        </ul>
                    </div>
                    <div class="implication-column">
                        <h4>Operational Implications</h4>
                        <ul class="implications-list">
                            <li onmouseover="showEnhancedTooltip(event, {
                                title: 'Operational Readiness',
                                description: 'Current operational readiness and capability levels.',
                                source: '<strong>📊 Sources:</strong><br/>• Operational Reports<br/>• Readiness Assessments<br/>• Training Data<br/>• Equipment Status',
                                strategic_impact: '<strong>🎯 Impact:</strong><br/>Direct impact on mission success and effectiveness.',
                                recommendations: 'Maintain high readiness levels through continuous training.',
                                use_cases: 'Operational planning and resource allocation.'
                            })" onmouseout="hideEnhancedTooltip()">
                                <strong>Operational Readiness</strong>
                                <p>Current operational readiness and capability levels</p>
                                <span class="impact-level">Medium Impact</span>
                            </li>
                            <li onmouseover="showEnhancedTooltip(event, {
                                title: 'Resource Allocation',
                                description: 'Optimal resource allocation for maximum effectiveness.',
                                source: '<strong>📊 Sources:</strong><br/>• Budget Analysis<br/>• Resource Planning<br/>• Cost-Benefit Studies<br/>• Efficiency Metrics',
                                strategic_impact: '<strong>🎯 Impact:</strong><br/>Efficient resource use maximizes operational effectiveness.',
                                recommendations: 'Optimize resource allocation based on priority needs.',
                                use_cases: 'Budget planning and resource management.'
                            })" onmouseout="hideEnhancedTooltip()">
                                <strong>Resource Allocation</strong>
                                <p>Optimal resource allocation for maximum effectiveness</p>
                                <span class="impact-level">Medium Impact</span>
                            </li>
                        </ul>
                    </div>
                </div>
            `;
            
            modalContent.innerHTML = content;
            modal.style.display = 'block';
            
            // Create chart after modal is shown
            setTimeout(() => {
                createChart(moduleId, moduleData.chartData);
            }, 100);
        }
        
        // Close Module Modal
        function closeModuleModal() {
            document.getElementById('moduleModal').style.display = 'none';
        }
        
        // Create Chart
        function createChart(moduleId, chartData) {
            const canvas = document.getElementById(`chart_${moduleId}`);
            if (!canvas) return;
            
            const ctx = canvas.getContext('2d');
            
            const config = {
                type: chartData.type,
                data: {
                    labels: chartData.labels,
                    datasets: [{
                        label: moduleDetails[moduleId].title,
                        data: chartData.data,
                        backgroundColor: chartData.type === 'radar' ? 'rgba(54, 162, 235, 0.2)' : 
                                        chartData.type === 'doughnut' ? [
                                            'rgba(255, 99, 132, 0.8)',
                                            'rgba(54, 162, 235, 0.8)',
                                            'rgba(255, 205, 86, 0.8)',
                                            'rgba(75, 192, 192, 0.8)',
                                            'rgba(153, 102, 255, 0.8)',
                                            'rgba(255, 159, 64, 0.8)'
                                        ] : 'rgba(54, 162, 235, 0.6)',
                        borderColor: chartData.type === 'radar' ? 'rgba(54, 162, 235, 1)' : 
                                   chartData.type === 'doughnut' ? '#fff' : 'rgba(54, 162, 235, 1)',
                        borderWidth: chartData.type === 'doughnut' ? 2 : 2,
                        tension: chartData.type === 'line' ? 0.4 : 0
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    scales: chartData.type === 'radar' ? {
                        r: {
                            beginAtZero: true,
                            max: 100
                        }
                    } : {
                        y: {
                            beginAtZero: true
                        }
                    }
                }
            };
            
            new Chart(ctx, config);
        }
        
        // Close modal when clicking outside
        window.onclick = function(event) {
            const modal = document.getElementById('moduleModal');
            if (event.target === modal) {
                closeModuleModal();
            }
        }
    </script>
</body>
</html>
