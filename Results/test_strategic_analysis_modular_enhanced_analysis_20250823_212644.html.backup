    </div>
    
    <script>
        
        // Enhanced tooltip functionality
        function showEnhancedTooltip(event, tooltipData) {
            const tooltip = document.getElementById('enhancedTooltip');
            const title = document.getElementById('tooltipTitle');
            const content = document.getElementById('tooltipContent');
            const source = document.getElementById('tooltipSource');
            const strategic = document.getElementById('tooltipStrategic');
            const recommendations = document.getElementById('tooltipRecommendations');
            const useCases = document.getElementById('tooltipUseCases');
            
            title.textContent = tooltipData.title;
            content.innerHTML = tooltipData.description;
            source.innerHTML = tooltipData.source;
            strategic.innerHTML = tooltipData.strategic_impact;
            
            if (tooltipData.recommendations) {
                recommendations.innerHTML = '<strong>üí° Recommendations:</strong><br/>' + tooltipData.recommendations;
                recommendations.style.display = 'block';
            } else {
                recommendations.style.display = 'none';
            }
            
            if (tooltipData.use_cases) {
                useCases.innerHTML = '<strong>üéØ Use Cases:</strong><br/>' + tooltipData.use_cases;
                useCases.style.display = 'block';
            } else {
                useCases.style.display = 'none';
            }
            
            tooltip.style.display = 'block';
            tooltip.style.left = event.pageX + 10 + 'px';
            tooltip.style.top = event.pageY - 10 + 'px';
        }
        
        function hideEnhancedTooltip() {
            document.getElementById('enhancedTooltip').style.display = 'none';
        }
        
        // Module scripts
        
        // Tooltip data for strategicrecommendationsmodule
        const strategicrecommendationsmoduleTooltipData = {"intelligence_summary": {"title": "Intelligence Analysis Summary", "description": "Comprehensive overview of intelligence insights and analysis results.", "source": "Source: Intelligence Analysis Framework", "strategic_impact": "Critical for understanding the strategic context and decision-making basis.", "recommendations": "\u2022 Review intelligence sources regularly\n\u2022 Validate insights with multiple sources\n\u2022 Update confidence scores based on new information", "use_cases": "\u2022 Strategic planning sessions\n\u2022 Intelligence briefings\n\u2022 Decision support systems\n\u2022 Risk assessment frameworks", "confidence": 95.0}, "confidence_metrics": {"title": "Confidence Metrics", "description": "Statistical measures of confidence in intelligence assessments and recommendations.", "source": "Source: Confidence Assessment Model", "strategic_impact": "Essential for risk assessment and decision-making prioritization.", "recommendations": "\u2022 Establish confidence thresholds\n\u2022 Implement confidence tracking systems\n\u2022 Regular confidence reassessment", "use_cases": "\u2022 Risk management\n\u2022 Decision prioritization\n\u2022 Resource allocation\n\u2022 Strategic planning", "confidence": 90.0}, "high_impact_insights": {"title": "High Impact Insights", "description": "Critical insights that have significant strategic implications.", "source": "Source: Impact Assessment Framework", "strategic_impact": "Direct influence on strategic decision-making and resource allocation.", "recommendations": "\u2022 Prioritize high-impact insights\n\u2022 Develop response strategies\n\u2022 Monitor impact evolution", "use_cases": "\u2022 Strategic planning\n\u2022 Crisis management\n\u2022 Resource allocation\n\u2022 Policy development", "confidence": 92.0}};
        
        // Initialize tooltips for strategicrecommendationsmodule
        document.querySelectorAll('[data-tooltip-strategicrecommendationsmodule]').forEach(element => {
            const tooltipId = element.getAttribute('data-tooltip-strategicrecommendationsmodule');
            const tooltipData = strategicrecommendationsmoduleTooltipData[tooltipId];
            
            if (tooltipData) {
                element.addEventListener('mouseenter', (e) => {
                    showEnhancedTooltip(e, tooltipData);
                });
                
                element.addEventListener('mouseleave', () => {
                    hideEnhancedTooltip();
                });
            }
        });
        
        
