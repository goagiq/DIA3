        <div class="chart-container">
            <h4>üö¢ Naval Capabilities Radar Chart</h4>
            <canvas id="naval_capabilities_balanceofpowermodule" width="400" height="400"></canvas>
            <script>
                const ctx_naval_capabilities_balanceofpowermodule = document.getElementById('naval_capabilities_balanceofpowermodule').getContext('2d');
                new Chart(ctx_naval_capabilities_balanceofpowermodule, {
                    type: 'radar',
                    data: {
                        labels: ['Surface Combatants', 'Submarines', 'Naval Aviation', 'Amphibious Forces', 'Anti-Submarine Warfare'],
                        datasets: [{'label': 'Surface Combatants', 'data': [70.0, 0, 0, 0, 0], 'backgroundColor': 'rgba(54, 162, 235, 0.2)', 'borderColor': 'rgba(54, 162, 235, 1)', 'borderWidth': 2}, {'label': 'Submarines', 'data': [0, 70.0, 0, 0, 0], 'backgroundColor': 'rgba(255, 99, 132, 0.2)', 'borderColor': 'rgba(255, 99, 132, 1)', 'borderWidth': 2}]
                    },
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        scales: {
                            r: {
                                beginAtZero: true,
                                max: 100
                            }
                        },
                        plugins: {
                            legend: {
                                position: 'top'
                            },
                            title: {
                                display: true,
